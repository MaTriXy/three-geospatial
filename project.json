{
  "name": "@geovanni/source",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --target=lint --parallel=8 --exclude=@geovanni/source"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx format:write --all"
      }
    }
  }
}
